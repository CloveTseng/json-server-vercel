    "paths": {
    "/users": {
        "get": {
            "summary": "取得所有使用者",
            "responses": {
                "200": {
                    "description": "成功取得使用者資料",
                    "content": {
                    "application/json": {
                    "schema": {
                        "type": "array",
                        "items": {
                        "type": "object",
                        "properties": {
                            "id": { "type": "integer" },
                            "name": { "type": "string" }
                        }
                    }
                    }
                }
                }
            }
            }
        }
        }
    }
